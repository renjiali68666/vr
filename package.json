{
  "name": "vite-vue",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@photo-sphere-viewer/core": "^5.5.1",
    "@photo-sphere-viewer/cubemap-adapter": "^5.5.1",
    "@photo-sphere-viewer/cubemap-tiles-adapter": "^5.5.1",
    "@photo-sphere-viewer/cubemap-video-adapter": "^5.5.1",
    "@photo-sphere-viewer/equirectangular-tiles-adapter": "^5.5.1",
    "@photo-sphere-viewer/equirectangular-video-adapter": "^5.5.1",
    "@photo-sphere-viewer/gyroscope-plugin": "^5.5.1",
    "@photo-sphere-viewer/little-planet-adapter": "^5.5.1",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/node": "^18.19.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "less": "^4.2.0",
    "npm-run-all2": "^6.1.1",
    "prettier": "^3.0.3",
    "typescript": "~5.3.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.25"
  }
}
